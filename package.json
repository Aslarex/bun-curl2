{
  "name": "bun-curl2",
  "version": "0.0.30",
  "author": {
    "name": "Nicholas",
    "url": "https://github.com/Aslarex",
    "email": "err.nicholas@gmail.com"
  },
  "repository": "https://github.com/Aslarex/bun-curl2",
  "keywords": [
    "bun",
    "curl",
    "fetch alternative",
    "fast fetch",
    "bun curl",
    "quick http",
    "http2 fetch",
    "http3 fetch"
  ],
  "description": "blazing fast, fetch-like HTTP client built with Bun and cURL in TypeScript.",
  "license": "WTFPL",
  "scripts": {
    "dev": "DEBUG=* bun test --watch",
    "build": "rm -rf dist && tsc --project tsconfig.json && esbuild --minify $(find ./dist -type f -name '*.js') --outdir=dist --tree-shaking=true --allow-overwrite --format=esm && rm -rf dist/types.js",
    "prettier": "prettier src/ tests/ examples/ --write"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "module": "./dist/index.js",
  "files": ["dist"],
  "types": "./dist/index.d.ts",
  "optionalDependencies": {
    "redis": "^4.0.0",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/node": "^20.0.0",
    "bun-types": "^1.1.0",
    "esbuild": "^0.25.0",
    "typescript": "^5.0.0"
  }
}